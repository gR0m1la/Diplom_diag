            <threeD>1</threeD> <!-- 3D -->
        </options>
        <params>
            <label>
                <font name="Arial" size="10" color="#000000"/>
            </label>
            <legend>
                <backgroundcolor>#EFEFEF</backgroundcolor> <!-- Цвет фона легенды -->
                <bordercolor>#CCCCCC</bordercolor> <!-- Цвет рамки легенды -->
