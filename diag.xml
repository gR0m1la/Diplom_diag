                <scale>100</scale> <!-- Масштаб (1% - 500%) -->
                <rotation>360</rotation> <!-- Поворот (0-360 градусов) -->
                <tilt>340</tilt> <!-- Наклон (270 - 360) -->
                <perspective>50</perspective> <!-- Перспектива (0-100) -->
            </threeD>
        </params>
    </object>
