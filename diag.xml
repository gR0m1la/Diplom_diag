<shema width="800" height="600" mnname="Новая мнемосхема 1" bkcolor="#FFFFFF">
    <object height="320" width="350" cy="50" cx="20" goid="0" type="TDiagram">
        <options>
            <diagramform>Pie</diagramform>
            <diagramtype>Historical</diagramtype> <!-- Тип диаграммы -->
            <historicaltype>Interpolated</historicaltype> <!-- Тип исторических запросов -->
            <updateperiod>10</updateperiod> <!-- Период обновления диаграммы в минутах -->
            <showlabels>1</showlabels> <!-- Показывать метки -->
            <showlegend>1</showlegend> <!-- Показывать легенду -->
            <showtitle>1</showtitle> <!-- Показывать заголовок -->
